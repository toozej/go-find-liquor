{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run go-find-liquor",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["--debug", "--config", "${workspaceFolder}/config.yaml", "--once"]
        },
        {
            "name": "Get go-find-liquor version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["version"]
        },
    ]
}